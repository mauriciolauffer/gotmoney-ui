{
  "_version": "1.5.0",
  "start_url": "../index.html",
  "sap.app": {
    "id": "com.mlauffer.gotmoneyappui5",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "2.1.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}"
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "sap-icon://lead",
      "favIcon": "images/favicon.ico",
      "phone": "images/pig-57x57.png",
      "phone@2": "images/pig-144x144.png",
      "tablet": "images/pig-72x72.png",
      "tablet@2": "images/pig-144x144.png"
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": [
      "sap_hcb",
      "sap_belize"
    ],
    "fullWidth": true
  },
  "sap.ui5": {
    "dependencies": {
      "minUI5Version": "1.46.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {},
        "sap.ui.unified": {},
        "sap.tnt": {},
        "openui5.validator": {}
      }
    },
    "resourceRoots": {
      "openui5.password": "./js/openui5/password/",
      "openui5.validator": "./js/openui5/validator/"
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.mlauffer.gotmoneyappui5.i18n.i18n"
        }
      },
      "": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "local": {
        "type": "com.mlauffer.gotmoneyappui5.model.LocalStorageModel"
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": false
    },
    "handleValidation": true,
    "rootView": {
      "viewName": "com.mlauffer.gotmoneyappui5.view.App",
      "id": "rootApp",
      "type": "XML",
      "async": true
    },
    "routing": {
      "config": {
        "async": true,
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "com.mlauffer.gotmoneyappui5.view",
        "controlId": "idAppControl",
        "controlAggregation": "pages",
        "transition": "slide",
        "homeRoute": "index",
        "bypassed": {
          "target": "notFound"
        }
      },
      "routes": [
        {
          "pattern": "",
          "name": "index",
          "target": "index"
        },
        {
          "pattern": "home",
          "name": "home",
          "target": "home"
        },
        {
          "pattern": "about",
          "name": "about",
          "target": "about"
        },
        {
          "pattern": "privacy",
          "name": "privacy",
          "target": "privacy"
        },
        {
          "pattern": "terms",
          "name": "terms",
          "target": "terms"
        },
        {
          "pattern": "user",
          "name": "profile",
          "target": "profile"
        },
        {
          "pattern": "signup",
          "name": "signup",
          "target": "signup"
        },
        {
          "pattern": "account",
          "name": "accountList",
          "target": "accountList"
        },
        {
          "pattern": "account/{accountId}",
          "name": "account",
          "target": "account"
        },
        {
          "pattern": "accountNew",
          "name": "accountNew",
          "target": "accountNew"
        },
        {
          "pattern": "category",
          "name": "categoryList",
          "target": "categoryList"
        },
        {
          "pattern": "category/{categoryId}",
          "name": "category",
          "target": "category"
        },
        {
          "pattern": "categoryNew",
          "name": "categoryNew",
          "target": "categoryNew"
        },
        {
          "pattern": "transaction",
          "name": "transactionList",
          "target": "transactionList"
        },
        {
          "pattern": "transactionOverdue",
          "name": "transactionOverdue",
          "target": "transactionOverdue"
        },
        {
          "pattern": "transaction/{transactionId}",
          "name": "transaction",
          "target": "transaction"
        },
        {
          "pattern": "transactionNew",
          "name": "transactionNew",
          "target": "transactionNew"
        }
      ],
      "targets": {
        "index": {
          "viewId": "index",
          "viewName": "Index",
          "viewLevel": 1
        },
        "home": {
          "viewId": "home",
          "viewName": "Home",
          "viewLevel": 1
        },
        "about": {
          "viewId": "about",
          "viewName": "About",
          "viewLevel": 2
        },
        "privacy": {
          "viewId": "privacy",
          "viewName": "Privacy",
          "viewLevel": 2
        },
        "terms": {
          "viewId": "terms",
          "viewName": "Terms",
          "viewLevel": 2
        },
        "notFound": {
          "viewId": "notFound",
          "viewName": "NotFound",
          "transition": "show"
        },
        "profile": {
          "viewId": "user",
          "viewName": "User",
          "viewLevel": 2
        },
        "signup": {
          "viewId": "signup",
          "viewName": "Signup",
          "viewLevel": 2
        },
        "accountList": {
          "viewId": "accountList",
          "viewName": "AccountList",
          "viewLevel": 2
        },
        "account": {
          "viewId": "account",
          "viewName": "Account",
          "viewLevel": 3
        },
        "accountNew": {
          "viewId": "accountNew",
          "viewName": "AccountNew",
          "viewLevel": 3
        },
        "categoryList": {
          "viewId": "categoryList",
          "viewName": "CategoryList",
          "viewLevel": 2
        },
        "category": {
          "viewId": "category",
          "viewName": "Category",
          "viewLevel": 3
        },
        "categoryNew": {
          "viewId": "categoryNew",
          "viewName": "CategoryNew",
          "viewLevel": 3
        },
        "transactionList": {
          "viewId": "transactionList",
          "viewName": "TransactionList",
          "viewLevel": 2
        },
        "transactionOverdue": {
          "viewId": "transactionOverdue",
          "viewName": "TransactionOverdue",
          "viewLevel": 2
        },
        "transaction": {
          "viewId": "transaction",
          "viewName": "Transaction",
          "viewLevel": 3
        },
        "transactionNew": {
          "viewId": "transactionNew",
          "viewName": "TransactionNew",
          "viewLevel": 3
        }
      }
    }
  }
}
