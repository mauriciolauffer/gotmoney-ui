                <Title level="H2" text="{i18n>Privacy.t6}" class="sapMH3FontSize sapUiLargeMarginTop"/>
                <Text text="{i18n>Privacy.p6}"/>
                <Title level="H2" text="{i18n>Privacy.t7}" class="sapMH3FontSize sapUiLargeMarginTop"/>
                <Text text="{i18n>Privacy.p7}"/>
                <Title level="H2" text="{i18n>Privacy.t8}" class="sapMH3FontSize sapUiLargeMarginTop"/>
                <HBox>
                    <Text text="{i18n>Privacy.p8} "/>
                    <Link text=" contato@gotmoneyapp.com" href="mailto:contato@gotmoneyapp.com"/>
                </HBox>
            </VBox>
        </Panel>
        <footer>
            <core:Fragment fragmentName="com.mlauffer.gotmoneyappui5.view.IndexFooter" type="XML"/>
        </footer>
    </Page>
</mvc:View>
