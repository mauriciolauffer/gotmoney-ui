            <VBox>
                <Title level="H2" text="{i18n>Privacy.t1}" class="sapMH3FontSize"/>
                <Text text="{i18n>Privacy.p1}"/>
                <Title level="H2" text="{i18n>Privacy.t2}" class="sapMH3FontSize sapUiLargeMarginTop"/>
